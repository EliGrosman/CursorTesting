{
  "version": 2,
  "name": "claude-clone-api",
  "builds": [
    {
      "src": "src/index.vercel.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/src/index.vercel.ts"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "src/index.vercel.ts": {
      "maxDuration": 30,
      "memory": 1024
    }
  }
}